CREATE TABLE articles(
  id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
  external_id TEXT NOT NULL UNIQUE,
  feed_id INTEGER REFERENCES feeds(id),
  title TEXT,
  content_html TEXT,
  url TEXT,
  summary TEXT,
  image_url TEXT,
  date_published INTEGER
);

findAll:
SELECT *
FROM articles;

create:
