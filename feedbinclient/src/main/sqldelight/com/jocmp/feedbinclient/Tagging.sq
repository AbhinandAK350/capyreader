CREATE TABLE IF NOT EXISTS Tagging (
  id INTEGER PRIMARY KEY NOT NULL,
  feed_id INTEGER NOT NULL,
  name TEXT NOT NULL
);

CREATE INDEX index_tagging_id ON Tagging(id);

all:
SELECT *
FROM Tagging
ORDER BY id;

insert:
INSERT OR REPLACE INTO Tagging(
  id,
  feed_id,
  name
)
VALUES (
  ?,
  ?,
  ?
);

deleteRemoved:
DELETE FROM Tagging
WHERE id NOT IN ?;
